SELECT NAME, MIN(YEAR), MAX(YEAR), COUNT(*) FROM STUDIO 
  JOIN MOVIE ON NAME = STUDIONAME AND NAME LIKE 'M%'
  GROUP BY NAME
  
SELECT NAME, COUNT(*) FROM MOVIESTAR
  JOIN STARSIN ON NAME=STARNAME AND GENDER = 'f'
  GROUP BY STARNAME
  HAVING COUNT(*) >= ALL(SELECT COUNT(*) FROM MOVIESTAR
  JOIN STARSIN ON NAME=STARNAME AND GENDER = 'f'
  GROUP BY STARNAME)
